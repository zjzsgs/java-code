	public Hero(String name){this.name = name;}
	public Hero(String name,float hp,int damage){this.name=name;this.hp=hp;this.damage=damage;}
	public int compareTo(Hero arg0) {
		// TODO Auto-generated method stub
		return 0;
	}
	
}
