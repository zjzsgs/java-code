package classic50;
/**
 * 打印水仙花数
 * 153=1^3+5^3+3^3
 * @author zjz
 *
 */
public class Test03 {
	public static void main(String[] args) {
		test1();
	}

	private static void test1() {
		int l,m,n;
		int sum=0;
		for (int i = 100; i < 1000; i++) {
			l=i/100;
			m=(i-100*l)/10;
			n=i%100;
			sum=l*l*l+m*m*m+n*n*n;
		}
	}

}
