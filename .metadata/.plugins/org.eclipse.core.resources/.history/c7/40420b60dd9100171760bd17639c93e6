package classic50;
/**
 * 求101-200之间的素数
 * 素数只有两个因子 1和本身
 * @author zjz
 *
 */
public class Test02 {
	public static void main(String[] args) {
	
		int num=0;
		int n=0;
		for(int i=101;i<=200;i++){
			int m=0;
			for (int j = 1; j <= i; j++) {
				n=i%j;
				if(n==0){
					m++;
				}
				if(m>2){
					break;
				}
			}
			if(m>2){
				System.out.println(i);
				++num;
			}
			
		}
		System.out.println(num);
	}
}
